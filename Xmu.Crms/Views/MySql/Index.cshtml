@model IEnumerable<Xmu.Crms.Models.Courses>
@{
    ViewBag.Title = null;
}
<h1>课程</h1>
<table class="table">
    <tr>
        <th>课程号</th>
        <th>课程名</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelitem => item.id)</td>
            <td>@Html.DisplayFor(modelitem => item.name)</td>
        </tr>
    }
</table>

